#define #(Null)()       0
#define #(Alloc)() (#0)XALLOC(sizeof(Voidstar), (tSDLTypeInfo *)&ySDL_#(Voidstarstar))
#define #(vstarstar2vstar)(P) ((#(Voidstar))P)
